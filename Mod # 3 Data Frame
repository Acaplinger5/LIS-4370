# Define the vectors
Name <- c("Jeb", "Donald", "Ted", "Marco", "Carly", "Hillary", "Bernie")
ABC_political_poll_results <- c(4, 62, 51, 21, 2, 14, 15)
CBS_political_poll_results <- c(12, 75, 43, 19, 1, 21, 19)

# Create the data frame
political_poll_df <- data.frame(
  Name = Name,
  ABC_political_poll_results = ABC_political_poll_results,
  CBS_political_poll_results = CBS_political_poll_results
)

# Display the data frame
print(political_poll_df)
